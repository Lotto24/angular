{
  "$schema": "http://json-schema.org/schema",
  "$id": "ScaffoldExamples",
  "title": "",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "description": "Path to JSON template",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "pattern": "\\.json$"
    },
    "namespace": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "What namespace should the examples components be created in?"
    }
  },
  "required": ["path", "namespace"]
}
